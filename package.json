  },
  "keywords": [
    "vue",
    "vue2",
    "vue3",
    "vue2/3",
    "vue-number-roll",
    "vue-demi"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "u": "unocss src/h.ts -o=dist/index.css",
