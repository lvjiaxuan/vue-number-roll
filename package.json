{
  "name": "vue-number-roll",
  "type": "module",
  "version": "2.0.1",
  "private": false,
  "author": "lvjiaxuan <471501748@qq.com> (https://github.com/lvjiaxuan)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lvjiaxuan/vue-number-roll"
  },
  "keywords": [
    "vue",
    "vue2",
    "vue3",
    "vue2/3",
    "vue-number-roll",
    "vue-demi"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:dev3": "nr build && ni -C=playgrounds/vue3 --force && nr dev3",
    "build:dev2": "nr build && ni -C=playgrounds/vue2.7 --force &&  nr dev2",
    "dev3": "nr -C=playgrounds/vue3 dev",
    "dev2": "nr -C=playgrounds/vue2.7 dev",
    "u": "unocss src/index.ts -o=dist/index.css",
    "stub": "unbuild --stub && (nr u & nr inject)",
    "build": "unbuild && (nr u & nr inject)",
    "inject": "nr inject:css & nr inject:pkg",
    "inject:css": "node ./scripts/injectCSS.mjs",
    "inject:pkg": "node ./scripts/injectPkg.mjs",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "prepare": "ni -C=playgrounds/vue3 & ni -C=playgrounds/vue2.7"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.7.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "vue-demi": "^0.14.5"
  },
  "devDependencies": {
    "@lvjiaxuan/eslint-plugin": "^0.15.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^20.2.1",
    "@unocss/cli": "^0.52.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.41.0",
    "unbuild": "^1.2.1",
    "unocss": "^0.52.0",
    "unocss-transformer-attribute-values-group": "^0.2.2",
    "vue": "^3.3.4"
  }
}
