    "dev3": "vue-demi-switch 3 && cd playgrounds/vue3 && nr dev",
    "build": "nlx unbuild && (nr u & node ./scripts/injectCSS.mjs)",
    "prepublishOnly": "nr build",
    "lint": "eslint .",
    "r": "lvr --no-push"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.7.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@unocss/reset": "^0.51.12",
    "vue-demi": "^0.14.1"
  },
  "devDependencies": {
    "@lvjiaxuan/eslint-plugin": "^0.12.0",
    "@tsconfig/node18": "^2.0.0",
    "@types/node": "^20.1.2",
    "@unocss/cli": "^0.51.12",
    "@vue/tsconfig": "^0.3.2",
    "esbuild": "^0.17.19",
